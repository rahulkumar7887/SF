<apex:page standardController="Candidate__c" showHeader="false" extensions="WizardController">
<apex:pageMessages ></apex:pageMessages>
    <apex:form >
        <apex:pageBlock title="Candidate Address info">
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!Candidate__c.City__c}"/>
                <apex:inputField value="{!Candidate__c.State_Province__c}"/>
                <apex:commandButton value="Create Candidate" action="{!createCandidate}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>