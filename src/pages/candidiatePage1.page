<apex:page standardController="Candidate__c" showHeader="false" extensions="WizardController">
<apex:pageMessages ></apex:pageMessages>
    <apex:form >
        <apex:pageBlock title="Candidate Personal info">
            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!Candidate__c.First_Name__c}"/>
                <apex:inputField value="{!Candidate__c.Last_Name__c}"/>
                
                <apex:inputField value="{!Candidate__c.Account__c}"/>
                <apex:commandButton value="Next screen" action="{!nextPage}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>